#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/@electron+asar@4.0.1/node_modules/@electron/asar/bin/node_modules:/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/@electron+asar@4.0.1/node_modules/@electron/asar/node_modules:/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/@electron+asar@4.0.1/node_modules/@electron/node_modules:/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/@electron+asar@4.0.1/node_modules:/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/@electron+asar@4.0.1/node_modules/@electron/asar/bin/node_modules:/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/@electron+asar@4.0.1/node_modules/@electron/asar/node_modules:/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/@electron+asar@4.0.1/node_modules/@electron/node_modules:/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/@electron+asar@4.0.1/node_modules:/home/omori/Schreibtisch/signal-styler/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@electron/asar/bin/asar.mjs" "$@"
else
  exec node  "$basedir/../@electron/asar/bin/asar.mjs" "$@"
fi
